{
    "manifest_version": 3,
    "name": "Video Speed Controller",
    "description": "Control video playback speed on any webpage.",
    "version": "1.0",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  